    keywords = {impedance-control, robotics, teaching},
    month = mar,
    pages = {1--24},
    posted-at = {2008-01-07 12:32:53},
    priority = {2},
    title = {{Impedance control - An approach to manipulation. I - Theory. II - Implementation. III - Applications}},
